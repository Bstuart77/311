(define list1 (list "a" "b" "c" "d" "a" "c" "a" "a"))
(define list2 (list "c" "c" "b" "e" "e" "e" "e" "a"))
(define list3 (list "a" "a" "b" "b" "b" "c" "b" "a" "b" "e"))
