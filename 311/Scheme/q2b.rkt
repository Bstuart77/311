(define list9 (list "a" "d" "e" 6 8 "e" "c" 3 6 291))
(define list10 (list 99 99 99 1 99 99 1))

(define (findAndReplace x y lst)
  (cond ((null? lst) '()) 
        ((list? (car lst)) (cons (findAndReplace x y (car lst)) (findAndReplace x y (cdr lst)))) 
        ((eq? (car lst) x) (cons y (findAndReplace x y (cdr lst))))
        (else
         (cons (car lst) (findAndReplace x y (cdr lst))))))

(findAndReplace "a" "c" list1)
